	</body>
		{% block footer %}
        <!-- Footer -->
        <footer class="bg-dark bg-primary text-center text-white" id="footer">
        <!-- Grid container -->
        <div class="container p-4">
            <!-- Section: Social media -->
            <section class="mb-4">
            <!-- Facebook -->
            <a class="btn btn-outline-light btn-floating m-1" href="https://www.facebook.com/UniversiteGustaveEiffel/" role="button"
                ><i class="fa fa-facebook-f"></i
            ></a>

            <!-- Twitter -->
            <a class="btn btn-outline-light btn-floating m-1" href="https://twitter.com/UGustaveEiffel" role="button"
                ><i class="fa fa-twitter"></i
            ></a>



            <!-- Instagram -->
            <a class="btn btn-outline-light btn-floating m-1" href="https://www.instagram.com/universitegustaveeiffel/" role="button"
                ><i class="fa fa-instagram"></i
            ></a>

            <!-- Linkedin 
            <a class="btn btn-outline-light btn-floating m-1" href="#!" role="button"
                ><i class="fa fa-linkedin-in"></i
            ></a>-->

            </section>
            <!-- Section: Social media -->

            <!-- Section: Form -->
            <section class="">
            <form action="">
                <!--Grid row-->
                <div class="row d-flex justify-content-center">
                <!--Grid column-->
                <div class="col-auto">
                    <p class="pt-2">
                    {#<strong>{% trans %} Sign up for our newsletter {% endtrans %}</strong>#}
                    </p>
                </div>
                <!--Grid column-->

                <!--Grid column-->
                <div class="col-md-5 col-12">
                    <!-- Email input -->
                    <div class="form-outline form-white mb-4">
                    {#<input type="email" id="form5Example2" class="form-control" />
                    <label class="form-label" for="form5Example2">{% trans %} Email address {% endtrans %}</label>
                    #}</div>
                </div>
                <!--Grid column-->

                <!--Grid column-->
                <div class="col-auto">
                    <!-- Submit button -->
                    {#}
                    <button type="submit" class="btn btn-outline-light mb-4">
                    {% trans %} Subscribe {% endtrans %}
                    
                    </button>#}
                </div>
                <!--Grid column-->
                </div>
                <!--Grid row-->
            </form>
            </section>
            <!-- Section: Form -->

            <!-- Section: Text -->
            <section class="mb-4">
            <p>
                {% trans %} Our partners {% endtrans %}
            </p>
            </section>
            <!-- Section: Text -->

            <!-- Section: Links -->
            <section class="">
            <!--Grid row-->
            <div class="row">
                <!--Grid column-->
                <div class="col-lg-4 col-md-6 mb-4 mb-md-0">
                <img src="{{asset('Icons/uge_logo.png')}}"  class="logo" id="logo2"/>

                </div>
                <!--Grid column-->

                <!--Grid column-->
                <div class="col-lg-4 col-md-6 mb-4 mb-md-0">
                <img src="{{asset('Icons/esa_logo.png')}}"  class="logo"/>
                </div>
                <!--Grid column-->

                <!--Grid column-->
                <div class="col-lg-4 col-md-6 mb-4 mb-md-0">
                <img src="{{asset('Icons/logo_ensg.png')}}"  class="logo"/>

                </div>
                <!--Grid column-->

                <!--Grid column-->
                <div class="col-lg-4 col-md-6 mb-4 mb-md-0">

                <img src="{{asset('Icons/vertigeo_logo.png')}}"  class="logo" id="logo4"/>

                </div>

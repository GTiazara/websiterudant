                <!--Grid column-->
                <div class="col-lg-4 col-md-6 mb-4 mb-md-0">

                <img src="{{asset('Icons/adat.jpg')}}"  class="logo" id="logo3"/>

                </div>
                <!--Grid column-->
            </div>
            <!--Grid row-->
            </section>
